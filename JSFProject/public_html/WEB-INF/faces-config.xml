<?xml version="1.0" encoding="windows-1256"?>
<faces-config version="2.1" xmlns="http://java.sun.com/xml/ns/javaee">
  <managed-bean>
    <managed-bean-name>LoginBean</managed-bean-name>
    <managed-bean-class>j2ee.jsf.beans.LoginBean</managed-bean-class>
    <managed-bean-scope>session</managed-bean-scope>
  </managed-bean>
  <navigation-rule>
    <from-view-id>*</from-view-id>
    <navigation-case>
      <from-outcome>About</from-outcome>
      <to-view-id>/HomePage.jsf</to-view-id>
    </navigation-case>
    <navigation-case>
      <from-outcome>Profile</from-outcome>
      <to-view-id>/pages/Profile.jsf</to-view-id>
    </navigation-case>
    <navigation-case>
      <from-outcome>Contact</from-outcome>
      <to-view-id>/pages/Contact.jsf</to-view-id>
    </navigation-case>
    <navigation-case>
      <from-outcome>Home</from-outcome>
      <to-view-id>/pages/HomePage.jsf</to-view-id>
    </navigation-case>
    <navigation-case>
      <from-outcome>Index</from-outcome>
      <to-view-id>/index.jsf</to-view-id>
    </navigation-case>
    <navigation-case>
      <from-outcome>About</from-outcome>
      <to-view-id>/pages/About.jsf</to-view-id>
    </navigation-case>
    <navigation-case>
      <from-outcome>Register</from-outcome>
      <to-view-id>/pages/Register.jsf</to-view-id>
    </navigation-case>
    <navigation-case>
      <from-outcome>ConfirmRegister</from-outcome>
      <to-view-id>/pages/ConfirmRegister.jsf</to-view-id>
    </navigation-case>
    <navigation-case>
      <from-outcome>Store</from-outcome>
      <to-view-id>/pages/Store.jsf</to-view-id>
    </navigation-case>
  </navigation-rule>
  <managed-bean>
    <managed-bean-name>RegisterBean</managed-bean-name>
    <managed-bean-class>j2ee.jsf.beans.RegisterBean</managed-bean-class>
    <managed-bean-scope>session</managed-bean-scope>
  </managed-bean>
  <navigation-rule>
    <from-view-id>/pages/ConfirmRegister.jsf</from-view-id>
    <navigation-case>
      <from-outcome>RegisterSuccess</from-outcome>
      <to-view-id>/pages/RegisterSuccess.jsf</to-view-id>
    </navigation-case>
  </navigation-rule>
  <managed-bean>
    <managed-bean-name>StoreBean</managed-bean-name>
    <managed-bean-class>j2ee.jsf.beans.StoreBean</managed-bean-class>
    <managed-bean-scope>session</managed-bean-scope>
  </managed-bean>
  <navigation-rule>
    <from-view-id>/pages/Store.jsf</from-view-id>
    <navigation-case>
      <from-outcome>ShowCart</from-outcome>
      <to-view-id>/pages/ShowCart.jsf</to-view-id>
    </navigation-case>
  </navigation-rule>
</faces-config>